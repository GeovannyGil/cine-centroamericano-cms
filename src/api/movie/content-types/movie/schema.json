{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Pel√≠culas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": true,
      "required": true
    },
    "synopsis": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "year": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": false
    },
    "duration": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "director": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "language": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "link_trailer": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text",
      "required": false
    },
    "link_movie": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text",
      "required": true
    },
    "genreds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genred.genred",
      "inversedBy": "movies"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "movies"
    },
    "movie_uid": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "sliders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slider.slider",
      "mappedBy": "movie"
    }
  }
}
